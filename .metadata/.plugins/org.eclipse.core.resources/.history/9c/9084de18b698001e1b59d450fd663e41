package inheritance;

public class Programmer {
	
	String name;
	String role;
	String email;
	String programmerCode;
	
	void writeCode() {
		System.out.println(name+" is writing code");
	}
	
	void thinkALgorithm() {
		System.out.println(name+" is thinking algorithm");
	}
	
	void testing() {
		System.out.println(name+" is testing");
	}
}
