package java_basic;

public class TernaryOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int x=20;
			int y=15;
			int z=30;
			
			int result1=0;
			int result2=0;
			
			
			if(x > y) {
				result1 =x;
			}
			else {
				result2=y;
			}
			
			if(y> z) {
				result2=y;
			}
			else {
				result2=z;
			}
	}

}
