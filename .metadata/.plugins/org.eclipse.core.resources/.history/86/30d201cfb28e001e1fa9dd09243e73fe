package java_basic;

public class VarArgsMethod {
	
	static int infinityMultiply(int[] numArray) {
		
		int result =1;
		for(int element : numArray ) {
			result *= element;
		}
		
		return result;
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] array1 = {10,20,30};
		int[] array2 = {10,20,30,40,50};
		int[] array3 = {10,20,11,22,33,44};
		
		System.out.println(VarArgsMethod.infinityMultiply(array1));
		System.out.println(VarArgsMethod.infinityMultiply(array2));
		System.out.println(VarArgsMethod.infinityMultiply(array3));
	}

}
