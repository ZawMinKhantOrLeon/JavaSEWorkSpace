package java_basic;

public class ArrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//declare
		
		int[] marks = new int[6];
		
		marks[0]=70;
		marks[1]=90;
		marks[2]=100;
		marks[3]=67;
		marks[4]=88;
		marks[5]=99;
		System.out.println(marks);
		System.out.println("index - 0 = "+marks[0]);
	}

}
