package polymorphism;

public class Main {

		
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Animal animal=new Cat();
		animal.makeSound();
		animal.eat();
		animal.walk();
		
		Cat cat= (Cat) animal;
		cat.jump();
		cat.scrapt();
		((Cat)animal).jump();
		((Cat)animal).scrapt();
		
		
		animal=new Dog();
		animal.makeSound();
		animal.eat();
		animal.walk();

	}

}
