package polymorphism;

public class Main {

		
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Animal animal=new Cat();
		animal.makeSound();
		animal.eat();
		animal.walk();
		
		((Cat)Animal).jump();
		((Cat)Animal).scrapt();
		
		
		animal=new Dog();
		animal.makeSound();
		animal.eat();
		animal.walk();

	}

}
