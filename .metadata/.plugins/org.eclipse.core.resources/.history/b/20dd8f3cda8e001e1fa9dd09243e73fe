package java_basic;

public class Employee {
	long id;
	String name;
	String email;
	String role;
	double salary;
	
	 Employee(){
		System.out.println("Inside Employee No Arguments Constructor");
	}
	
	void update(long updateId,String updateName,String updateEmail,String updateRole,double updateSalary) {
		 id=updateId;
		 name=updateName;
		 email=updateEmail;
		 role=updateRole;
		 salary=updateSalary;
	}
}
