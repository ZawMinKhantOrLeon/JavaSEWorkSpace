package java_basic;

public class Car {
	
	static int numberOfWheels;
	
	String brand = "Toyota";
	String model;
	double price;
	int year;
	int speedPerKM;
	
	void  engineStart() {
		System.out.printf("%s's engine is starting",model);
	}
	
	void  drive() {
		System.out.printf("%s's car is driving",model);
	}
	
	void  brake() {
		System.out.printf("%s's car is braked",model);
	}
	
	void  horn() {
		System.out.printf("%s's car is horning",model);
	}
}

