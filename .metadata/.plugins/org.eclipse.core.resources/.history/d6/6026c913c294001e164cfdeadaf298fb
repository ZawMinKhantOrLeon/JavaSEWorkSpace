package final_modifier;

 class FakeDatabase {
	
	final static Authentication[] database = {
			
			new Authentication("MgMg","1234"),
			new Authentication("MaSapal","apple"),
			new Authentication("Kyaw","kyaw11"),
			new Authentication("Aung","2468"),
			new Authentication("Kaung","a123")
	};
	
	static Authentication authenticate(String username,String password) {
		Authentication check=0;
		for(final Authentication auth: database) {
			if(username == auth.username && password == auth.password) {
				check = auth;
			}
		}
		return check;
	}
}
